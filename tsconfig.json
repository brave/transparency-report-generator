{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ES2022",
        "rootDir": "src",
        "moduleResolution": "node",
        "outDir": "dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "skipLibCheck": true,
        "sourceMap": true
    }
}
